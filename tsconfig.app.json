{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["@angular/localize"],
    // Update this part
    "paths": {
      "app/*": ["./src/app/*"],
      "images/*": ["./src/assets/images/*"],
      "environment/*": ["./src/environments/*"]
